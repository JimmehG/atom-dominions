scopeName: "source.dominions"
name: "Dominions"
fileTypes: ["dm"]
limitLineLength: false
patterns: [
    #Repositories.
    {
        include: "#comment"
    },

    {
        include: "#mod"
    },

    {
        include: "#record"
    }
]

repository:
    comment:
        patterns: [
            {
                match: "--(.*)$"
                name: "comment.line.double-dash.dominions"
            }
        ]

    #A. Global mod commands.
    mod:
        patterns: [
            {
                match: '^\\s*(#modname)\\s+("[^"]+")'
                captures:
                    1:
                        name: "keyword.control.mod.modname.dominions"
                    2:
                        name: "string.quoted.double.mod.modname.dominions"
                name: "meta.mod.modname.dominions"
            },

            {
                begin: '^\\s*(#description)\\s+(")'
                captures:
                    1:
                        name: "keyword.control.mod.description.dominions"
                    2:
                        name: "punctuation.definition.string.begin.dominions"
                end: '"'
                contentName: "string.quoted.double.mod.description.dominions"
                endCaptures:
                    0:
                        name: "punctuation.definition.string.end.dominions"
                name: "meta.mod.description.dominions"
            },

            {
                match: "^\\s*(#(version|domversion))\\s+(\\d+\\.(?:\\d+{1,2}))"
                captures:
                    1:
                        name: "keyword.control.mod.$2.dominions"
                    3:
                        name: "constant.other.mod.$2.dominions"
                name: "meta.mod.$2.dominions"
            },

            {
                match: '^\\s*(#icon)\\s+("[^"]+")'
                captures:
                    1:
                        name: "keyword.control.mod.icon.dominions"
                    2:
                        name: "string.quoted.double.mod.icon.dominions"
                name: "meta.mod.icon.dominions"
            },

            #Repositories.
            {
                include: "#mod_nation"
            },

            {
                include: "#mod_spell"
            },

            {
                include: "#mod_item"
            },

            {
                include: "#mod_global"
            },

            {
                include: "#mod_merc"
            }
        ]

    #Global record-like definitions.
    record:
        patterns: [
            #A. Weapons.
            {
                comment: "As of v5.24, selection weapon numbers vary between 0 and 1999."
                begin: '^\\s*(#selectweapon)\\s+(?:("[^"]+")|(1[0-9]{3}|[1-9][0-9]{0,2}|0))'
                beginCaptures:
                    1:
                        name: "keyword.control.record.selectweapon.dominions"
                    2:
                        name: "string.quoted.double.record.selectweapon.dominions"
                    3:
                        name: "constant.numeric.record.selectweapon.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.selectweapon.end.dominions"
                contentName: "meta.record.selectweapon.body.dominions"
                name: "meta.record.selectweapon.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#weapon"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            {
                comment: "As of v5.24, new weapon numbers vary between 700 and 1999."
                begin: "^\\s*(#newweapon)\\s+(1[0-9]{3}|[7-9][0-9]{2})\\b"
                beginCaptures:
                    1:
                        name: "keyword.control.record.newweapon.dominions"
                    2:
                        name: "constant.numeric.record.newweapon.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.newweapon.end.dominions"
                contentName: "meta.record.newweapon.body.dominions"
                name: "meta.record.newweapon.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#weapon"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            #B. Armors.
            {
                comment: "As of v5.24, selection armor numbers vary between 0 and 999."
                begin: '^\\s*(#selectarmor)\\s+(?:("[^"]+")|([1-9][0-9]{0,2}|0))'
                beginCaptures:
                    1:
                        name: "keyword.control.record.selectarmor.dominions"
                    2:
                        name: "string.quoted.double.record.selectarmor.dominions"
                    3:
                        name: "constant.numeric.record.selectarmor.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.selectarmor.end.dominions"
                contentName: "meta.record.selectarmor.body.dominions"
                name: "meta.record.selectarmor.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#armor"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            {
                comment: "As of v5.24, new armor numbers vary between 300 and 999."
                begin: "^\\s*(#newarmor)\\s+([3-9][0-9]{2}|2[5-9][0-9])\\b"
                beginCaptures:
                    1:
                        name: "keyword.control.record.newarmor.dominions"
                    2:
                        name: "constant.numeric.record.newarmor.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.newarmor.end.dominions"
                contentName: "meta.record.newarmor.body.dominions"
                name: "meta.record.newarmor.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#armor"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            #C. Monsters.
            {
                comment: "As of v5.24, selection monster numbers vary between 0 and 8999."
                begin: '^\\s*(#selectmonster)\\s+(?:("[^"]+")|([1-8][0-9]{3}|[1-9][0-9]{0,2}|0))'
                beginCaptures:
                    1:
                        name: "keyword.control.record.selectmonster.dominions"
                    2:
                        name: "string.quoted.double.record.selectmonster.dominions"
                    3:
                        name: "constant.numeric.record.selectmonster.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.selectmonster.end.dominions"
                contentName: "meta.record.selectmonster.body.dominions"
                name: "meta.record.selectmonster.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#monster_item"
                    },

                    {
                        include: "#monster"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            {
                comment: "As of v5.24, new monster numbers vary between 3500 and 8999."
                begin: "^\\s*(#newmonster)(?:\\s+([4-8][0-9]{3}|3[5-9][0-9]{2}))?"
                beginCaptures:
                    1:
                        name: "keyword.control.record.newmonster.dominions"
                    2:
                        name: "constant.numeric.record.newmonster.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.newmonster.end.dominions"
                contentName: "meta.record.newmonster.body.dominions"
                name: "meta.record.newmonster.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#monster_item"
                    },

                    {
                        include: "#monster"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            #D. Nametypes.
            {
                comment: "As of v5.24, nametype numbers vary between 100 and 299. Usable numbers start at 165."
                begin: "^\\s*(#selectnametype)\\s+([1-2][0-9]{2})\\b"
                beginCaptures:
                    1:
                        name: "keyword.control.record.selectnametype.dominions"
                    2:
                        name: "constant.numeric.record.selectnametype.dominions"
                end: "^(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.selectnametype.end.dominions"
                contentName: "meta.record.selectnametype.body.dominions"
                name: "meta.record.selectnametype.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#nametype"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            #E. Sites.
            {
                comment: "As of v5.24, site numbers vary between 0 and 1999."
                begin: '^\\s*(#selectsite)\\s+(?:("[^"]+")|(1[0-9]{3}|[1-9][0-9]{0,2}|0))'
                beginCaptures:
                    1:
                        name: "keyword.control.record.selectsite.dominions"
                    2:
                        name: "string.quoted.double.record.selectsite.dominions"
                    3:
                        name: "constant.numeric.record.selectsite.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.selectsite.end.dominions"
                contentName: "meta.record.selectsite.body.dominions"
                name: "meta.record.selectsite.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#site_nation"
                    },

                    {
                        include: "#site"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            {
                comment: "As of v5.24, site numbers vary between 1500 and 1999."
                begin: "^\\s*(#newsite)(?:\\s+(1[5-9][0-9]{2}))?"
                beginCaptures:
                    1:
                        name: "keyword.control.record.newsite.dominions"
                    2:
                        name: "constant.numeric.record.newsite.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.newsite.end.dominions"
                contentName: "meta.record.newsite.body.dominions"
                name: "meta.record.newsite.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#site_nation"
                    },

                    {
                        include: "#site"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            #F. Nations.
            {
                comment: "As of v5.24, nation numbers vary between 5 and 249, with numbers 0-4 hardcoded for independents."
                begin: "^\\s*(#(selectnation))\\s+(2[0-4][0-9]|1[0-9]{2}|[1-9][0-9]?|0)\\b"
                beginCaptures:
                    1:
                        name: "keyword.control.record.selectnation.dominions"
                    3:
                        name: "constant.numeric.record.selectnation.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.selectnation.end.dominions"
                contentName: "meta.record.selectnation.body.dominions"
                name: "meta.record.selectnation.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#site_nation"
                    },

                    {
                        include: "#nation_poptype"
                    },

                    {
                        include: "#nation"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            {
                comment: "To make new nation with fixed number, use #selectnation with an id higher than 120."
                begin: "^\\s*(#newnation)"
                beginCaptures:
                    1:
                        name: "keyword.control.record.newnation.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.newnation.end.dominions"
                contentName: "meta.record.newnation.body.dominions"
                name: "meta.record.newnation.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#site_nation"
                    },

                    {
                        include: "#nation_poptype"
                    },

                    {
                        include: "#nation"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            #G. Spells.
            {
                comment: "As of v5.24, spell numbers vary between 0 and 3999."
                begin: '^\\s*(#selectspell)\\s+(?:("[^"]+")|([1-3][0-9]{3}|[1-9][0-9]{0,2}|0))'
                beginCaptures:
                    1:
                        name: "keyword.control.record.selectspell.dominions"
                    2:
                        name: "string.quoted.double.record.selectspell.dominions"
                    3:
                        name: "constant.numeric.record.selectspell.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.selectspell.end.dominions"
                contentName: "meta.record.selectspell.body.dominions"
                name: "meta.record.selectspell.dominions"
                patterns: [
                    {
                        include: "#spell"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            {
                comment: "As of v5.24, new spell numbers vary between 1300 and 3999."
                begin: "^\\s*(#newspell)(?:\\s+([2-3][0-9]{3}|1[3-9][0-9]{2}))?"
                beginCaptures:
                    1:
                        name: "keyword.control.record.newspell.dominions"
                    2:
                        name: "constant.numeric.record.newspell.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.newspell.end.dominions"
                name: "meta.record.newspell.dominions"
                patterns: [
                    {
                        include: "#spell"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            #H. Items.
            {
                comment: "As of v5.24, item numbers vary between 0 and 999. New item numbers start at 500."
                begin: '^\\s*(#selectitem)\\s+(?:("[^"]+")|([1-9][0-9]{0,2}|0))'
                beginCaptures:
                    1:
                        name: "keyword.control.record.selectitem.dominions"
                    2:
                        name: "string.quoted.double.record.selectitem.dominions"
                    3:
                        name: "constant.numeric.record.selectitem.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.selectitem.end.dominions"
                contentName: "meta.record.selectitem.body.dominions"
                name: "meta.record.selectitem.dominions"
                patterns: [
                    {
                        include: "#monster_item"
                    },

                    {
                        include: "#item"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            {
                begin: "^\\s*(#newitem)"
                beginCaptures:
                    1:
                        name: "keyword.control.record.newitem.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.newitem.end.dominions"
                contentName: "meta.record.newitem.body.dominions"
                name: "meta.record.newitem.dominions"
                patterns: [
                    {
                        include: "#monster_item"
                    },

                    {
                        include: "#item"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            #I. Poptypes.
            #note: as of v5.24, *known* poptype numbers vary between 25 and 99.
            {
                begin: "^\\s*(#selectpoptype)\\s+(\\d+)"
                beginCaptures:
                    1:
                        name: "keyword.control.record.selectpoptype.dominions"
                    2:
                        name: "constant.numeric.record.selectpoptype.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.selectpoptype.end.dominions"
                contentName: "meta.record.selectpoptype.body.dominions"
                name: "meta.record.selectpoptype.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#nation_poptype"
                    },

                    {
                        include: "#poptype"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            #J. Mercenaries.
            {
                begin: "^\\s*(#newmerc)"
                beginCaptures:
                    1:
                        name: "keyword.control.record.newmerc.dominions"
                end: "^(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.newmerc.end.dominions"
                contentName: "meta.record.newmerc.body.dominions"
                name: "meta.record.newmerc.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#merc"
                    },

                    {
                        include: "#comment"
                    }
                ]
            },

            #K. Events.
            {
                begin: "^\\s*(#newevent)"
                beginCaptures:
                    1:
                        name: "keyword.control.record.newevent.dominions"
                end: "^\\s*(#end)"
                endCaptures:
                    1:
                        name: "keyword.control.record.newevent.end.dominions"
                contentName: "meta.record.newevent.body.dominions"
                name: "meta.record.newevent.dominions"
                patterns: [
                    #Repositories.
                    {
                        include: "#event"
                    },

                    {
                        include: "#comment"
                    }
                ]
            }
        ]

    weapon:
        patterns: [
            #Basic commands.
            {
                match: "^\\s*(#clear)"
                captures:
                    1:
                        name: "keyword.control.weapon.clear.dominions"
                name: "meta.weapon.clear.dominions"
            },

            {
                match: "^\\s*(#copyweapon)\\s+(1[0-9]{3}|[1-9][0-9]{0,2}|0)\\b"
                captures:
                    1:
                        name: "keyword.control.weapon.copyweapon.dominions"
                    2:
                        name: "constant.numeric.weapon.copyweapon.dominions"
                name: "meta.weapon.copyweapon.dominions"
            },

            #A. Basic properties.
            {
                match: '^\\s*(#name)\\s+("[^"]+")'
                captures:
                    1:
                        name: "keyword.control.weapon.name.dominions"
                    2:
                        name: "string.quoted.double.weapon.name.dominions"
                name: "meta.weapon.name.dominions"
            },

            {
                match: "^\\s*(#(twohanded|natural))"
                captures:
                    1:
                        name: "keyword.control.weapon.$2.dominions"
                name: "meta.weapon.$2.dominions"
            },

            {
                match: "^\\s*(#(dmg|nratt|att|def|prec|range))\\s+(-?\\d+)"
                captures:
                    1:
                        name: "keyword.control.weapon.$2.dominions"
                    3:
                        name: "constant.numeric.weapon.$2.dominions"
                name: "meta.weapon.$2.dominions"
            },

            {
                match: "^\\s*(#len)\\s+(-1|[0-6])\\b"
                captures:
                    1:
                        name: "keyword.control.weapon.len.dominions"
                    2:
                        name: "constant.numeric.weapon.len.dominions"
                name: "meta.weapon.len.dominions"
            },

            {
                comment: "As of v5.24, maximum ammo value is 30."
                match: "^\\s*(#ammo)\\s+(30|[1-2][0-9]|[0-9])\\b"
                captures:
                    1:
                        name: "keyword.control.weapon.ammo.dominions"
                    2:
                        name: "constant.numeric.weapon.ammo.dominions"
                name: "meta.weapon.ammo.dominions"
            },

            {
                match: "^\\s*(#rcost)\\s+(\\d+)"
                captures:
                    1:
                        name: "keyword.control.weapon.rcost.dominions"
                    2:
                        name: "constant.numeric.weapon.rcost.dominions"
                name: "meta.weapon.rcost.dominions"
            },

            #Sound properties.
            {
                match: '^\\s*(#sample)\\s+("[^"]+")'
                captures:
                    1:
                        name: "keyword.control.weapon.sample.dominions"
                    2:
                        name: "string.quoted.double.weapon.sample.dominions"
                name: "meta.weapon.sample.dominions"
            },

            {
                match: "^\\s*(#sound)\\s+(\\d+)"
                captures:
                    1:
                        name: "keyword.control.weapon.sound.dominions"
                    2:
                        name: "constant.numeric.weapon.sound.dominions"
                name: "meta.weapon.sound.dominions"
            },

            #B. Damtypes.
            {
                match: "^\\s*(#(dt_normal|dt_poison|dt_demon|dt_small|dt_magic|dt_large|dt_constructonly|dt_raise|dt_cap|dt_weakness|dt_holy|dt_drain|dt_sizestun|dt_weapondrain|dt_stun|dt_realstun|dt_interrupt|dt_bouncekill|dt_paralyze|dt_aff))"
                captures:
                    1:
                        name: "keyword.control.weapon.damtype.$2.dominions"
                name: "meta.weapon.damtype.$2.dominions"
            },

            #C. Damage qualifiers.
            {
                match: "^\\s*(#(poison|acid|slash|pierce|blunt|cold|fire|shock|magic|armorpiercing|armornegating|nostr|bowstr|halfstr|fullstr|mrnegates|mrnegateseasily|hardmrneg|sizeresist|mind|undeadimmune|inanimateimmune|flyingimmune|enemyimmune|friendlyimmune|undeadonly|demononly|sacredonly|demonundead|internal))"
                captures:
                    1:
                        name: "keyword.control.weapon.damqualifier.$2.dominions"
                name: "meta.weapon.damqualifier.$2.dominions"
            },

            #D. Other properties.
            {
                match: "^\\s*(#(bonus|ironweapon|woodenweapon|iceweapon|charge|flail|norepel|unrepel|beam|range050|range0|melee50|skip|skip2|uwok|nouw))"
                captures:
                    1:
                        name: "keyword.control.weapon.other.$2.dominions"
                name: "meta.weapon.other.$2.dominions"
            },

            {
                match: "^\\s*(#aoe)\\s+(\\d+)"
                captures:
                    1:
                        name: "keyword.control.weapon.other.aoe.dominions"
                    2:
                        name: "constant.numeric.weapon.other.aoe.dominions"
                name: "meta.weapon.other.aoe.dominions"
            },

            {
                match: "^\\s*(#(secondaryeffect|secondaryeffectalways))\\s+(\\d+)"
                captures:
                    1:
                        name: "keyword.control.weapon.other.$2.dominions"
                    3:
                        name: "constant.numeric.weapon.effect.dominions"
                name: "meta.weapon.other.$2.dominions"
            },

            #Visuals.
            {
                comment: "As of v5.24, explspr values vary between 10000 and 10317."
                match: "^\\s*(#explspr)\\s+(-1|10(?:[0-2][0-9][0-9]|30[0-9]|31[0-7]))\\b"
                captures:
                    1:
                        name: "keyword.control.weapon.other.explspr.dominions"
                    2:
                        name: "constant.numeric.weapon.other.explspr.dominions"
                name: "meta.weapon.other.explspr.dominions"
            },

            {
                match: "^\\s*(#flyspr)\\s+(-1|\\d+)(?:\\s+(\\d+))?"
                captures:
                    1:
                        name: "keyword.control.weapon.other.flyspr.dominions"
                    2:
                        name: "constant.numeric.weapon.other.flyspr.dominions"
                    3:
                        name: "constant.numeric.weapon.other.animlen.dominions"
                name: "meta.weapon.other.flyspr.dominions"
            }
        ]

    armor:
        patterns: [
            #Basic commands.
            {
                match: "^\\s*(#clear)"
                captures:
                    1:
                        name: "keyword.control.armor.clear.dominions"
                name: "meta.armor.clear.dominions"
            },

            {
                match: "^\\s*(#copyarmor)\\s+([1-9][0-9]{0,2}|0)\\b"
                captures:
                    1:
                        name: "keyword.control.armor.copyarmor.dominions"
                    2:
                        name: "constant.numeric.armor.copyarmor.dominions"
                name: "meta.armor.copyarmor.dominions"
            },

            #A. Basic Properties.
            {
                match: '^\\s*(#name)\\s+("[^"]+")'
                captures:
                    1:
                        name: "keyword.control.armor.name.dominions"
                    2:
                        name: "string.quoted.double.armor.name.dominions"
                name: "meta.armor.name.dominions"
            },

            {
                match: "^\\s*(#type)\\s+([4-6]|9)\\b"
                captures:
                    1:
                        name: "keyword.control.armor.type.dominions"
                    2:
                        name: "constant.numeric.armor.type.dominions"
                name: "meta.armor.type.dominions"
            },

            {
                match: "^\\s*(#(magic|iron|wooden)armor)"
                captures:
                    1:
                        name: "keyword.control.armor.$2.dominions"
                name: "meta.armor.$2.dominions"
